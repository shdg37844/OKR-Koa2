.page-container {
  height:100%;
  display: flex;
  flex-direction: column;
  padding:40rpx 35rpx;
}

.add-btn {
  padding:20rpx;
  text-align: center;
  color:#fff;
  background-color: #333;
}

.add-btn_hover {
  background-color: #666;
}

.okr-list {
  display: flex;
  flex-direction: column;
  gap:15rpx;
  margin-top:25rpx;
}

.okr-item {
  padding:15rpx;
  background-color: #f5f5f5;
}

.o-content {
  font-size: 16px;
  font-weight: 700;
}

.o-time {
  margin-left:15rpx;
  font-size:14px;
  color:#999;
}

