.page-container {
  height:100%;
  display: flex;
  flex-direction: column;
  padding:40rpx 35rpx;
}

.tk-title {
  font-size: 14px;
  color:#b4b5b4;
}

.tk-obj {
  margin:15rpx 0;
  font-size: 18px;
}

.tk-KR {
  display: flex;
  flex-direction: column;
  gap:20rpx;
}

.KR-item {
  padding:6rpx;
  border:1px solid #333;
}

.KR-item.active {
  color:#fff;
  background-color: #333;
}