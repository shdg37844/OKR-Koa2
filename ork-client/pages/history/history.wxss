.page-container {
  height:100%;
  display: flex;
  flex-direction: column;
  padding:40rpx 35rpx;
}

.todo-title {
  font-size: 14px;
  color:#b4b5b4;
}

.done-list {
  display: flex;
  flex-direction: column;
  gap:40rpx;
  margin-top:25rpx;
}

.done-item {
  position: relative;
  background-color: #f5f5f5;
}

.done-pic-box {
  position: absolute;
  top:-25rpx;
  right:0;
  width: 100rpx;
  height: 100rpx;
}

.done-pic {
  width: 100%;
  height: 100%;
}

.done-content {
  font-size: 16px;
  font-weight: 700;
}

.done-time {
  margin-left:15rpx;
  font-size:14px;
  color:#999;
}