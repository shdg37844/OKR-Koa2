.page-container {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding:60rpx;
}
.background-image {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
}


.title {
  margin-top: 30rpx;
  margin-bottom: 80rpx;
}

.cn-title {
  font-size: 28px;
  font-weight: 500;
}

.en-subtitle {
  font-weight: 500;
}

.content {
  font-size: 14px;
  color:#bcbcbc;
  font-weight: 600;
}

.button {
  margin-bottom: 140rpx;
  line-height: 110rpx;
  text-align: center;
  background-color: #333;
  color: #fff;
}

.button_hover {
 background-color: #666;
}